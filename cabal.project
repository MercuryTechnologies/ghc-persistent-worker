packages: ./internal
          ./instrument
          ./buck-worker
          ./buck-multiplex-worker

-- For MWB-customized GHC, turn this on.
-- flags: +mwb

package buck-worker
  executable-dynamic: true

package buck-multiplex-worker
  executable-dynamic: true

allow-newer:
  proto-lens:base

if (impl(ghc >= 9.11))
  allow-newer:
    time,
  constraints:
    extra == 1.8

index-state: 2025-04-02T00:00:00Z
